{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "build" : {
            "dependsOn": ["^build"],
            "outputs": [
                "dist/**"
            ] 
        },
        "deploy": {
            "cache": false,
            "dependsOn": ["build"]
        },
        "test": {
            "dependsOn": ["build"]
        }
    }
}